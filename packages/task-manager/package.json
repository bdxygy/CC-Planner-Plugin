{
  "name": "@architect-planner/task-manager",
  "version": "0.0.6",
  "description": "Task Manager CLI for architect-planner",
  "type": "module",
  "main": "../plugin/scripts/task-manager/index.cjs",
  "bin": {
    "task-manager": "../../plugin/scripts/task-manager/index.cjs"
  },
  "scripts": {
    "typecheck": "tsc --noEmit",
    "build": "rsbuild build && chmod +x ../../plugin/scripts/task-manager/index.cjs",
    "dev": "rsbuild build --watch",
    "clean": "rm -rf ../../plugin/scripts/task-manager"
  },
  "dependencies": {
    "@architect-planner/types": "workspace:*",
    "@architect-planner/utils": "workspace:*",
    "cli-table3": "^0.6.5",
    "commander": "^12.0.0",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@rsbuild/core": "^1.7.2",
    "typescript": "~5.9.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "cli",
    "task-manager",
    "ndjson"
  ],
  "author": "Budi Santoso",
  "license": "MIT"
}
